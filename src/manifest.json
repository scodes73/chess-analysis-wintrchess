{
    "manifest_version": 3,
    "name": "Chess Analysis WintrChess",
    "description": "Analyze your chess.com games on WintrChess for free. ",
    "version": "0.0.1",
    "icons": {
        "64": "img/64.png",
        "128": "img/128.png",
        "512": "img/512.png"
    },
    "background": {
        "service_worker": "background.js"
    },
    "incognito": "split",
    "permissions": ["declarativeContent", "activeTab", "scripting"],
    "host_permissions": ["https://www.chess.com/*", "https://wintrchess.com/*"],
    "action": {
        "default_title": "Analyze on Wintrchess",
        "default_icon": {
            "16": "img/16.png",
            "32": "img/32.png",
            "48": "img/48.png",
            "64": "img/64.png",
            "128": "img/128.png",
            "512": "img/512.png"
        }
    }
}
